{
  "name": "orangehrm-opensource",
  "version": "1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier src --write",
    "format-test": "prettier __tests__ --write"
  },
  "dependencies": {
    "@react-native-community/async-storage": "~1.11.0",
    "@react-native-community/masked-view": "~0.1.10",
    "@react-native-community/slider": "~3.0.0",
    "@react-navigation/drawer": "~5.8.2",
    "@react-navigation/native": "~5.5.0",
    "@react-navigation/stack": "~5.5.1",
    "native-base": "~2.13.12",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-calendars": "~1.300.0",
    "react-native-gesture-handler": "~1.6.1",
    "react-native-modal": "~11.5.6",
    "react-native-reanimated": "~1.9.0",
    "react-native-safe-area-context": "~3.0.2",
    "react-native-screens": "~2.8.0",
    "react-native-svg": "~12.1.0",
    "react-native-vector-icons": "~6.6.0",
    "react-redux": "~7.2.0",
    "redux": "~4.0.5",
    "redux-saga": "~1.1.3",
    "reselect": "~4.0.0",
    "victory-native": "~34.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "@types/jest": "^24.0.24",
    "@types/react-native": "^0.62.0",
    "@types/react-native-calendars": "^1.20.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "16.9.2",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "^2.0.4",
    "react-test-renderer": "16.11.0",
    "typescript": "^3.8.3",
    "utility-types": "^3.10.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testRegex": [
      "./__tests__/[^setup].*.tsx$"
    ],
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js",
      "./__tests__/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|native-base|victory-.*|@react-navigation/.*)"
    ]
  }
}
